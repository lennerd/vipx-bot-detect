{
    "name": "vipx/bot-detect",
    "type": "symfony-bundle",
    "description": "Bot detector library",
    "keywords": ["Bot"],
    "homepage": "http://lennerd.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennart Hildebrandt",
            "email": "hello@lennerd.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/yaml": "~2.1",
        "symfony/config": "~2.1"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": { "Vipx\\BotDetect": "" }
    },
    "target-dir": "Vipx/BotDetect",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
